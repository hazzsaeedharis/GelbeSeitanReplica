# Production Environment Variables for Railway

# Database - Supabase PostgreSQL with PostGIS
DATABASE_URL=postgresql://postgres.PROJECT_ID:PASSWORD@aws-1-eu-central-1.pooler.supabase.com:6543/postgres

# CORS - Add your Vercel frontend URL
CORS_ORIGINS=["https://gelbe-seitan-replica.vercel.app"]

# Security - Generate a secure random string (32+ characters)
SECRET_KEY=change-this-to-a-secure-random-string-32-chars-minimum

# Optional Services (set to False for now)
USE_ELASTICSEARCH=False
USE_REDIS_CACHE=False

# Application Settings
DEBUG=False
LOG_LEVEL=INFO
